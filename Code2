#include <iostream>
#include <cmath>

int main() {
    double length;
    
    std::cout << "Введите длинну окружности: ";
    std::cin >> length;
    
    double radius = length / (2 * M_PI);
    
    double area = M_PI * pow(radius, 2);
    
    std::cout << "Радиус: " << radius << std::endl;
    std::cout << "Площад круга: " << area << std::endl;
    
    return 0;
}
